# endsnippet

# superscripts
snippet sq "" i
^2
endsnippet

snippet cu "" i
^3
endsnippet


# fractions
priority 100
snippet // "" i
\\frac{${1:${VISUAL}}}{$2}$0
endsnippet

priority 200
snippet '((\d+)|(\d*)(\\)?([A-Za-z]+)((\^|_)(\{\d+\}|\d))*)//' "" wrA
\\frac{`!p snip.rv = match.group(1)`}{$1}$0
endsnippet


# environments
snippet begin
\\begin{${1:environment}}
	${0:${VISUAL}}
\\end{$1}
endsnippet

snippet center
\\begin{center}
	${0:${VISUAL}}
\\end{center}
endsnippet

snippet item
\\begin{itemize}
	\\item ${0:${VISUAL}}
\\end{itemize}
endsnippet

snippet desc
\\begin{description}
	\\item[$1] ${0:${VISUAL}}
\\end{description}
endsnippet

snippet enum
\\begin{enumerate}
	\\item ${0:${VISUAL}}
\\end{enumerate}
endsnippet

snippet align
\\begin{align${1:ed}}
	${0:${VISUAL}}
\\end{align$1}
endsnippet

snippet table
\\begin{table}[H]
	\\centering
	${0:${VISUAL}}
\\end{table}
endsnippet

snippet tab
\\begin{table}[H]
	\\centering
	\\begin{tabular}{${1:c}}
		${0:${VISUAL}}
	\\end{tabular}
\\end{table}
endsnippet

snippet taby
\\begin{table}[H]
	\\centering
	\\begin{tabulary}{${1:\\textwidth}}{${2:c}}
		${0:${VISUAL}}
	\\end{tabulary}
\\end{table}
endsnippet

snippet fig
\\begin{figure}[H]
	\\centering
	\\includegraphics[$1]{$0}
\\end{figure}
endsnippet

snippet frame
\\begin{frame}{$1}
	${0:${VISUAL}}
\\end{frame}
endsnippet

snippet block
